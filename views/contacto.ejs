<%- include('header') -%>
<%- include('navbar') -%>

<div class="container mt-5">
    <form>
        <div class="form-group row">
            <label for="nombre" class="col-sm-2 col-form-label">NOMBRE</label>
            <div class="col-sm-9">
                <input type="text" id="nombre" name="nombre" class="form-control"
                    placeholder="Ingrese su nombre y apellido">

            </div>

        </div>
        <div class="form-group row">
            <label for="email" class="col-sm-3 col-form-label">EMAIL</label>
            <div class="col-sm-9">
                <input type="email" id="email" name="email" class="email" placeholder="email@example.com">

            </div>

        </div>

        <div class="form-group row">
            <label for="email" class="col-sm-3 col-form-label">COMENTARIOS/SUGERENCIAS</label>
            <div class="col-sm-9">
                <textarea type="text" id="comentario" name="comentario" class="comentario"
                    placeholder="ingrese su comentario" rows="9">
                    </textarea>

            </div>

        </div>
        <div class="text-center mb-2">
            <button type="button" class="btn btn-dark">ENVIAR</button>

        </div>

    </form>
</div>
<%- include('pie') -%>